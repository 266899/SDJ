package t1;
import java.util.Scanner;
public class t2 {
	public static void main (String[] args) {
		int [] array =  {1,2,3,4};
		
		int first = array[0];
		for (int i = 0; i< array.length; i++)
		{
			System.out.print(array[i] + " ");
		}
		System.out.println();
		for (int i = 0; i< array.length; i++)
		{
			for (int j = 0; j<array.length; j++)
			{
				array[i]= array[j+1];
				
			}
			
		}
		for (int i = 0; i< array.length; i++)
		{
			System.out.println(array[i] + " ");
		}
		
	}

}

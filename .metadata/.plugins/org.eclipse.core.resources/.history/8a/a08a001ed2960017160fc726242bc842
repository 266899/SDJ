import java.util.ArrayList;

public class ticTacToe 
{
	public static void main (String[] args)
	{
		char[][] board = {{'o', ' ', ' '}, {' ', 'x', ' '}, {' ',' ',' '}};
	
		for (int i=0; i<2; i++)
		{
			System.out.print(board[0][i] + " | ");
		}
		System.out.println("----------");
		for (int i=0; i<2; i++)
		{
			System.out.print(board[1][i] + " | ");
		}
	}
}
